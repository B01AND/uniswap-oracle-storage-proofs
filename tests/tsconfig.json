{
	"compilerOptions": {
		"module": "CommonJS",
		"target": "ESNext",
		"moduleResolution": "node",
		"sourceMap": true,
		"inlineSources": true,
		"declaration": true,
		"noEmit": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
	},
	"include": [
		"**/*.ts"
	],
}
